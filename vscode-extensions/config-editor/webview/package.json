{
  "name": "vscode-config-editor-webview",
  "version": "11.2.0",
  "private": true,
  "dependencies": {
    "@axonivy/config-editor": "~11.2.0-next.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "vite": "^4.3.9"
  },
  "scripts": {
    "build": "vite build --mode development --emptyOutDir",
    "build:production": "vite build --emptyOutDir",
