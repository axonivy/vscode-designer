{
  "name": "vscode-inscription-extension",
  "private": true,
  "version": "11.2.0",
  "publisher": "AxonIvy",
  "repository": {
    "type": "git",
    "url": "https://github.com/axonivy/next-gen-inscription.git"
  },
  "displayName": "Axon Ivy Inscription View",
  "preview": true,
  "icon": "assets/ivy-logo.png",
  "engines": {
    "vscode": "^1.54.0"
  },
  "dependencies": {
    "@eclipse-glsp/vscode-integration-webview": "1.1.0-RC06",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "vscode-inscription-webview": "11.2.0",
    "@eclipse-glsp/vscode-integration": "1.1.0-RC06",
    "@types/node": "^16.7.13",
    "@types/vscode": "^1.54.0",
    "@types/vscode-webview": "^1.54.0",
    "vsce": "1.79.5"
  },
  "scripts": {
    "build": "tsc -p ./",
    "build:production": "yarn build",
    "clean": "rimraf build tsconfig.tsbuildinfo",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "package": "vsce package --yarn",
    "prepare": "yarn clean && yarn build",
    "watch": "tsc -w"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./lib/inscription-extension.js",
  "files": [
    "dist",
    "css",
    "src",
    "lib"
  ],
  "extensionDependencies": [
    "axonivy.vscode-process-editor-extension"
  ],
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "ivy-view-container",
          "title": "Ivy",
          "icon": "assets/ivy-logo.svg"
        }
      ]
    },
    "views": {
      "ivy-view-container": [
        {
          "type": "webview",
          "id": "inscriptionEditor",
          "name": "Inscription Editor"
        }
      ]
    }
  }
}
