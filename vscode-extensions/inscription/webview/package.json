{
  "name": "vscode-inscription-webview",
  "version": "11.2.0",
  "private": true,
  "dependencies": {
    "@axonivy/inscription-core": "~11.2.0-next.408",
    "@axonivy/inscription-editor": "~11.2.0-next.408",
    "@axonivy/inscription-protocol": "~11.2.0-next.408",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "vite": "^4.0.1",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "scripts": {
    "build": "vite build --mode development --emptyOutDir",
    "build:production": "vite build --emptyOutDir",
    "clean": "rimraf build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "prepare": "yarn clean && yarn build",
    "watch": "vite build --mode development --watch --emptyOutDir"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
