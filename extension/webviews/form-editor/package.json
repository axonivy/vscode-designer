  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "dependencies": {
    "@axonivy/form-editor": "~11.3.0-next.82",
    "@axonivy/form-editor-core": "~11.3.0-next.82",
