{
  "name": "vscode-process-editor-webview",
  "private": true,
  "version": "12.0.0",
  "description": "VSCode extension webview for the Axon Ivy (GLSP-based) process editor",
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "keywords": [
    "vscode",
    "graph",
    "diagram",
    "layout",
    "glsp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/axonivy/glsp-editor-client"
  },
  "dependencies": {
    "@axonivy/process-editor": "~12.0.0-next.1167",
    "@axonivy/process-editor-inscription": "~12.0.0-next.1167",
